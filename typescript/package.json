{
  "name": "trivia",
  "version": "1.0.0",
  "description": "Trivia kata in typescript",
  "main": "src/game-runner.ts",
  "scripts": {
    "start": "ts-node src/game-runner.ts",
    "test": "jest"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "typings"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "text",
      "html"
    ]
  },
  "author": "Felix Blechschmitt",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "nyc": "^15.1.0",
    "ts-node": "^7.0.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "ts-jest": "^28.0.4"
  }
}
